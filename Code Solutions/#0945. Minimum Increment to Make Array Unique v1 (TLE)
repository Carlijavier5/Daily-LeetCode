#0945. Minimum Increment to Make Array Unique v1 (TLE)

class Solution:
    def minIncrementForUnique(self, nums: List[int]) -> int:
        nums = sorted(nums);
        i = 0;
        count = 0;
        while i < len(nums):
            up = 1;
            try:
                while nums[i] == nums[i+up]:
                    up += 1;
            except:
                if i == len(nums)-1:
                    break;
            for x in range(i+1,i+up):
                count += 1;
                nums[x] += 1;
            i += 1;
            up = 1;
        return count;
